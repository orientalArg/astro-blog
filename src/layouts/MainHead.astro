---
import Seo from '../components/seo.astro'

const {title, description, image, frontmatter, robots } = Astro.props ;
---
<head>
    <meta charset="UTF-8">
    <link rel="icon" type="image/svg+xml" href="https://joaquinreynoso.vercel.app/@orientalArg.png" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="generator" content={Astro.generator} />
    <meta name="desciption" content={description} />
    <title>{title}</title>
    <Seo {title} {description} url={Astro.url} {image} {frontmatter} {robots} />
</head>